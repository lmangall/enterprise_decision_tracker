{
  "compilerOptions": {
    "jsx": "preserve",
    "types": [
      "react",
      "node",
      "cypress"
    ],
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "src/**/*",
    "src/*",
    ".next/types/**/*.ts",
    "cypress/**/*.ts",
    "src/components/.EditDecision.ts",
    "src/components/.DeleteDecision.ts"
, "src/components/.AddDecisionHandler"  ],
  "exclude": [
    "node_modules"
  ]
}
